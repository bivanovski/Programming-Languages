class Player {
	String name;
	int points;
	String team;
	
	public Player(String name, String team) {
		this.name = name;
		this.team = team;
		this.points = (int) (Math.random()*100) % 50;
	}
	
	public Player() {}
	
	void setName(String n) {
		name = n;
	}
	
	String getName()
	{
		return name;
	}
	
	void setPoints(int p)
	{
		points = p;
	}
	
	int getPoints () {
		return points;
	}
	
	Player Mvp(Player arr[]) {
		int max = 0;
		Player mvpPlayer = new Player();
		for(int i=0; i<arr.length; i++)
		{
			if(arr[i].getPoints() > max)
			{
				mvpPlayer = arr[i];
				return arr[i];
			}
		}
		return mvpPlayer;
	}
	
}

